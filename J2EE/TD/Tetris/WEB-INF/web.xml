    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <display-name>Mes servlets</display-name>
    <description>
      Premieres servlets
    </description>
	<servlet>
		<servlet-name>ServletTetris</servlet-name> 
		<servlet-class>servlet.ServletTetris</servlet-class> 
		<description>Servlet du score Tetris</description> 
	</servlet>
	<servlet-mapping> 
		<servlet-name>ServletTetris</servlet-name>
		<url-pattern>/tetris</url-pattern> 
	</servlet-mapping>
</web-app>